[ui]
username = Tim Taylor <tim@tool-man.org>
ignore.user = ~/.hgignore
editor = edit -w --new-window

[extensions]
hgext.extdiff=
hgext.rebase=

[extdiff]
cmd.vdiff=/Users/tim-taylor/bin/fmdiff

[extdiff]
cmd.foxdiff=/usr/bin/diff
opts.foxdiff=--unified=5 --ignore-space-change --new-file

[merge-tools]
filemerge.executable=/Users/tim-taylor/bin/fmdiff3
filemerge.args=-m $local $base $other > $output
filemerge.checkconflicts = true
filemerge.checkchanged = true
filemerge.gui = true
