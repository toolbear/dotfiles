[user]
    name = Tim Taylor
    email = tim@tool-man.org

[core]
    autocrlf = input
    safecrlf = true

[alias]
    co = checkout
    wc = whatchanged
    ci = commit
    st = status
    hist = log --pretty=format:\"%C(yellow)%h%Creset %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p

[color]
    ui = auto
[color "diff"]
    meta = bold
    frag = cyan ul
    old = cyan dim
